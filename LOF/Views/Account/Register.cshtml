@model LOF.RegisterTbl

@{
    Layout = null;
}


<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Sign Up Form </title>

    <!-- Font Icon -->
    <link rel="stylesheet" href=@Url.Content("~/Content/registrationform/fonts/material-icon/css/material-design-iconic-font.min.css")>
    <link rel="stylesheet" href=@Url.Content("~/Content/registrationform/vendor/nouislider/nouislider.min.css")>

    <!-- Main css -->
    <link rel="stylesheet" href=@Url.Content("~/Content/registrationform/css/style.css")>
    <link rel="stylesheet" href=@Url.Content("~/Content/cssmenu/styles.css")>
    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    <script src=@Url.Content("~/Content/cssmenu/script.js")></script>
    <link href="@Url.Content("~/Content/mytemplate/themes/css/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
</head>

<div id='cssmenu'>
    <ul>
        <li class='active'><a href="@Url.Content("~/Template/Index")">Lost & Found</a></li>
        <li><a href="@Url.Content("~/FoundProducts/Create")">Found Anything??</a></li>
        <li><a href="@Url.Content("~/LostProducts/Create")">Lost Anything??</a></li>
        <li><a href="@Url.Content("~/AllProducts/Index")">All Products</a></li>
        <li><a href="@Url.Content("~/FoundProducts/Index")">Found Products</a></li>
        <li><a href="@Url.Content("~/LostProducts/Index")">Lost Products</a></li>



    </ul>
    <ul class="nav navbar-nav navbar-right">

        <li><a class="navbar-brand active" href="@Url.Content("~/Account/Register")">Register</a></li>
        <li><a class=" active" href="@Url.Content("~/Account/Login")">Login</a></li>


    </ul>
</div>

<div class="main">

    <div class="container">
        <div class="signup-content">
            <div class="signup-img">
                <img src="@Url.Content("~/Content/registrationform/images/form-img.jpg")">
                <div class="signup-img-content">
                    <h2>Register now</h2>
                </div>
            </div>
            <div class="signup-form">
                <form method="POST" class="register-form" id="register-form">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-row">




                            <div class="form-group">

                                @Html.ValidationSummary(true)
                                <div class="form-input">
                                    <label for="first_name" class="required">Email</label>

                                    @Html.TextBoxFor(model => model.Email, new { @class = "form-control input-lg", name = "first_name" ,id = "first_name" })
                                    @Html.ValidationMessageFor(model => model.Email)
                                </div>

                                <div class="form-input">
                                    <label for="last_name" class="required">User name</label>

                                    @Html.TextBoxFor(model => model.UserName, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.UserName)

                                </div>

                                <div class="form-input">
                                    <label for="last_name" class="required">Password</label>

                                    @Html.TextBoxFor(model => model.Password, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.Password)
                                </div>

                                <div class="form-input">
                                    <label for="last_name" class="required">Repeat Password</label>

                                    @Html.TextBoxFor(model => model.ConfirmPassword, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.ConfirmPassword)
                                </div>
                            </div>
                            <div class="form-group">




                                <div class="form-input">
                                    <label for="last_name" class="required">Contact No</label>
                                    @Html.TextBoxFor(model => model.ContactNo, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.ContactNo)
                                </div>

                                <div class="form-input">
                                    <label for="last_name" class="required">Address</label>
                                    @Html.TextBoxFor(model => model.Address, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.Address)
                                </div>

                                <div class="form-input">
                                    <label for="last_name" class="required">Alternative Contact No</label>
                                    @Html.TextBoxFor(model => model.AlternativeContactNo, new { @class = "form-control input-lg" })
                                    @Html.ValidationMessageFor(model => model.AlternativeContactNo)
                                </div>
                            </div>

                            @* <div class="form-group">
                                    <div class="col-md-offset-2 col-md-10">
                                        <input type="submit" value="Create" class="btn btn-default" />
                                    </div>
                                </div>
                            *@
                        </div>
                        <div class="donate-us">
                            <label>Donate us</label>
                            <div class="price_slider ui-slider ui-slider-horizontal">
                                <div id="slider-margin"></div>
                                <span class="donate-value" id="value-lower"></span>
                            </div>
                        </div>
                        <div class="form-submit">
                            <input type="submit" value="Create" class="submit" id="submit" name="submit" />
                            @Html.ActionLink("Reset", "Register", "Account",null, new { @class="submit", value = "Reset", id = "reset" ,name = "reset" })

                        </div>
                    }

                </form>
            </div>

        </div>
    </div>
</div>
<script src=@Url.Content("~/Content/registrationform/vendor/jquery/jquery.min.js")></script>
<script src=@Url.Content("~/Content/registrationform/vendor/nouislider/nouislider.min.js")></script>
<script src=@Url.Content("~/Content/registrationform/vendor/wnumb/wNumb.js")></script>
<script src=@Url.Content("~/Content/registrationform/vendor/jquery-validation/dist/jquery.validate.min.js")></script>
<script src=@Url.Content("~/Content/registrationform/vendor/jquery-validation/dist/additional-methods.min.js")></script>
<script src=@Url.Content("~/Content/registrationform/js/main.js")></script>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
