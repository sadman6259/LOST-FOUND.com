                            <hr />
                            <div class="row row-space">

                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Owner Name</label>

                                        @Html.TextBoxFor(model => model.OwnerName, new { @class = "input--style-4", @Value = ViewBag.username })
                                        @Html.ValidationMessageFor(model => model.OwnerName)
                                    </div>
                                </div>
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Cell No</label>

                                        @Html.TextBoxFor(model => model.CellNo, new { @class = "input--style-4", @Value = ViewBag.cellno })
                                        @Html.ValidationMessageFor(model => model.CellNo)
                                    </div>
                                </div>
                                <div class="col-2">
                                    <div class="input-group">
                                        <label class="label">Addrerss</label>

                                        @Html.TextBoxFor(model => model.OwnerAddress, new { @class = "input--style-4", @Value = ViewBag.address })
                                        @Html.ValidationMessageFor(model => model.OwnerAddress)
                                    </div>
                                </div>
                            </div>

                            <div class="p-t-15">
                                <br />


                                <input type="submit" id="" value="Create" class="btn btn--radius-2 btn--blue" />
                            </div>



                        </form>
                    </div>
                </div>
            </div>
        </div>
    }
    <script src=@Url.Content("~/Content/regform/vendor/jquery/jquery.min.js")></script>
    <!-- Vendor JS-->
    <script src=@Url.Content("~/Content/regform/vendor/select2/select2.min.js")></script>
    <script src=@Url.Content("~/Content/regform/vendor/datepicker/moment.min.js")></script>
    <script src=@Url.Content("~/Content/regform/vendor/datepicker/daterangepicker.js")></script>
    <!-- Main JS-->
    <script src=@Url.Content("~/Content/regform/js/global.js")></script>
    

    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
</body>
</html>